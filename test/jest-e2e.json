{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "sharedTypes_shaka/utils-&-shared-types/(.*)": "<rootDir>/../libs/utils-&-shared-types/src/$1",
    "sharedTypes_shaka/utils-&-shared-types": "<rootDir>/../libs/utils-&-shared-types/src"
  }
}